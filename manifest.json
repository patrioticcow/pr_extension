{
  "name": "Phoneroutes",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "World Wide Phone Numbers",
  "homepage_url": "https://phoneroutes.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Phoneroutes",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "notifications",
    "http://*/*",
    "https://*/*"
  ]
}